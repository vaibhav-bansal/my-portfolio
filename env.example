# PostHog Analytics Configuration
# Copy this file to .env.local and fill in your actual values
# Get these values from your PostHog project settings

VITE_PUBLIC_POSTHOG_KEY=your_posthog_project_key_here
VITE_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Example values (replace with your actual values):
# VITE_PUBLIC_POSTHOG_KEY=phc_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# VITE_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# GTM Configuration (if using Google Tag Manager)
VITE_PUBLIC_GTM_AUTH_LIVE=your_production_gtm_auth_here
VITE_PUBLIC_GTM_PREVIEW_LIVE=env-1
VITE_PUBLIC_GTM_AUTH_LOCAL=your_development_gtm_auth_here
VITE_PUBLIC_GTM_PREVIEW_LOCAL=env-4

# Sanity CMS Configuration
# Get these values from your Sanity project settings
VITE_SANITY_PROJECT_ID=your_sanity_project_id_here
VITE_SANITY_DATASET=production
